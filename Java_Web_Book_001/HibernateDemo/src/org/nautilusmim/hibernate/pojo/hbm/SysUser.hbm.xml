<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.nautilusmim.hibernate.pojo.SysUser" table="sys_user" lazy="false">
		<id name="id" column="user_id" type="long">
			<generator class="native" />
		</id>
		<property name="name" column="name" type="string" length="32" />
		<property name="password" column="password" type="string" length="200" />
		<property name="type" column="type" type="short" />
	</class>
</hibernate-mapping>